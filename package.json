{
  "name": "typing",
  "version": "0.1.0",
  "description": "Typing!",
  "scripts": {
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "lint": "eslint --ignore-path .gitignore .",
    "server": "nodemon server/server.js --ignore client/",
    "server:prod": "NODE_ENV=production server/server.js",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --progress --colors",
    "e2e:ci": "NODE_ENV=tests nightwatch -e saucelabs-ie-9,saucelabs-ie-10,saucelabs-ie-11,saucelabs-edge,saucelabs-chrome-win,saucelabs-chrome-linux,saucelabs-chrome-mac,saucelabs-firefox-win,saucelabs-firefox-linux,saucelabs-firefox-mac,saucelabs-safari-mac",
    "e2e": "NODE_ENV=tests nightwatch -e chrome",
    "clean": "rm build -rf"
  },
  "license": "MIT",
  "author": "goldylucks <adamgoldman7@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/goldylucks/typing.git"
  },
  "bugs": {
    "url": "https://github.com/goldylucks/typing/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "6.0.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.9.0",
    "babel-runtime": "^6.5.0",
    "chai": "3.5.0",
    "chromedriver": "^2.21.2",
    "css-loader": "^0.23.1",
    "deasync": "^0.1.7",
    "eslint": "^2.13.1",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "ignore-styles": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma-babel-preprocessor": "6.0.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "mock-local-storage": "1.0.2",
    "morgan": "^1.7.0",
    "nightwatch": "^0.9.5",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.8.1",
    "pre-git": "^3.8.1",
    "rucksack-css": "^0.8.5",
    "selenium-server-standalone-jar": "^2.53.0",
    "sinon": "1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "cors": "^2.7.1",
    "event-emitter": "^0.3.4",
    "express": "^4.14.0",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "method-override": "^2.3.6",
    "mongodb": "^2.2.1",
    "mongoose": "^4.5.3",
    "raw-loader": "^0.5.1"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "scripts/protect-master.sh"
      ],
      "pre-push": [
        "npm run lint"
      ],
      "post-commit": [],
      "post-checkout": "npm install",
      "post-merge": "npm install"
    }
  }
}
