{
  "name": "typing",
  "version": "0.1.0",
  "description": "Typing!",
  "scripts": {
    "test": "npm run test:server",
    "test:server": "NODE_ENV=testing mocha server/**/*.spec.js",
    "start": "npm run client",
    "lint": "eslint --ignore-path .gitignore .",
    "client": "webpack --progress --colors -w",
    "client:devserver": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors -port 3001",
    "server": "nodemon server/server.js",
    "server:prod": "NODE_ENV=production node server/server.js",
    "server:ci": "NODE_ENV=ci node server/server.js",
    "prebuild:client": "npm run clean",
    "build:client": "webpack --progress --colors",
    "e2e:ci": "nightwatch -e saucelabs-chrome-win,saucelabs-chrome-linux,saucelabs-chrome-mac,saucelabs-firefox-win,saucelabs-firefox-linux,saucelabs-firefox-mac,saucelabs-safari-mac",
    "e2e": "nightwatch -e chrome",
    "clean": "rm build -rf"
  },
  "license": "MIT",
